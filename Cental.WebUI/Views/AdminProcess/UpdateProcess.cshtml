@model UpdateProcessDto
@{
    ViewData["Title"] = "UpdateProcess";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}

<form method="post">

    <input type="hidden" asp-for="ProcessId" class="form-control" />

    <label>Başlık</label>
    <input type="text" asp-for="Title" class="form-control" />
    <br />
    <label>Açıklama</label>
    <input type="text" asp-for="Description" class="form-control" />
    <br />
    

    <button class="btn btn-outline-primary">Kaydet</button>
</form>

