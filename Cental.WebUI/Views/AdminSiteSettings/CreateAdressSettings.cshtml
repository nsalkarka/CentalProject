
@model CreateSiteSettingsAdressDto
@{
    ViewData["Title"] = "CreateAdressSettings";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}

<h1>CreateAdressSettings</h1>


<form method="post" asp-action="CreateAdressSettings">
   
    <label>Adres Başlığı</label>
    <input type="text" asp-for="Title" class="form-control" />
    <br />
    <label>Adres Bilgileri</label>
    <input type="text" asp-for="Adress" class="form-control" />
    <br />
    <label>Harita Konumu</label>
    <input type="text" asp-for="MapLocation" class="form-control" />
    <br />
   


    <button class="btn btn-outline-primary">Kaydet</button>
</form>
